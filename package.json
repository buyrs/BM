{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "test": "vitest --run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:unit": "vitest --run tests/js/Components/**/*.test.js",
        "test:integration": "vitest --run tests/js/Integration/**/*.test.js",
        "test:e2e": "vitest --run tests/js/E2E/**/*.test.js",
        "test:mobile": "vitest --run tests/js/Mobile/**/*.test.js",
        "test:performance": "vitest --run tests/js/Performance/**/*.test.js",
        "test:comprehensive": "node tests/js/test-runner.js",
        "test:coverage": "vitest --run --coverage"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.2",
        "@tailwindcss/vite": "^4.0.0",
        "alpinejs": "^3.15.0",
        "autoprefixer": "^10.4.2",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "jsdom": "^25.0.1",
        "laravel-vite-plugin": "^2.0.1",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.1.0",
        "vite": "^7.0.0",
        "vitest": "^2.1.8"
    },
    "dependencies": {
        "@alpinejs/collapse": "^3.15.0",
        "@alpinejs/focus": "^3.15.0",
        "@tiptap/extension-color": "^3.3.1",
        "@tiptap/extension-highlight": "^3.3.1",
        "@tiptap/extension-placeholder": "^3.3.1",
        "@tiptap/extension-text-style": "^3.3.1",
        "@tiptap/extension-underline": "^3.3.1",
        "@tiptap/starter-kit": "^3.3.1",
        "chart.js": "^4.5.0",
        "date-fns": "^4.1.0",
        "flowbite": "^3.1.2",
        "tw-elements": "^2.0.0",
        "vite-plugin-pwa": "^1.0.0"
    }
}
